	@PersistenceContext
	EntityManager em;

    /**
     * Default constructor. 
     */
    public UserEJB() {
        // TODO Auto-generated constructor stub
    }
    
    public boolean AuthenticateUser(UserDTO user) {
    	em.find(arg0, arg1)
    }

}
